@page
@using Microsoft.AspNetCore.Http
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


  <div class="box">
           @if(TempData["Message1"] != null)
            {
                <div class="alert  bg-warning text-dark" role="alert">
                      <strong>@TempData["Message1"]</strong>
                </div>
            }
            @if(TempData["AlertMessage"] != null)
            {
                <div class="alert bg-warning">
                    <strong>@TempData["AlertMessage"]</strong>
                </div>

            }       
       <div class="form-box">

          <h3>Login In Here</h3>
        <form method="post">
          <div class="waiter"> 
            <label for="FirstName">Username </label>
            <input name="FirstName" placeholder="Enter Username" type="text"  asp-for="@Model.FirstName"/>
            <br>

            <div class="button">
            <button class="btn" type="submit" value="Login">Login</button>
            </div>
          </div>
         
        
               
        </form>
    
       </div>             
        
  </div>

       